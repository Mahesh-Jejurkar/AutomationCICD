<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

	<groups>
		<run>
			<include name="Purchase"></include>
		</run>
	</groups>

	<listeners>
		<listener class-name="seleniumframeworkdesign.testcomponents.Listeners"></listener>
	</listeners>

	<test name="Submit Order Test">
		<classes>
			<class name="seleniumframeworkdesign.test.SubmitOrderTest" />
		</classes>
	</test>

	<test name="Error Validation Test">
		<classes>
			<class name="seleniumframeworkdesign.test.ErrorValidationTest" />
		</classes>
	</test>

</suite>
